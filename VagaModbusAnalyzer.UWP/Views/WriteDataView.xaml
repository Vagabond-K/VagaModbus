                                        <TextBlock x:Uid="Text_Send"/>
                                    </ToolTipService.ToolTip>
                                </Button>
                                <Button Content="&#xE8C8;" Command="{StaticResource CopyToClipboardCommand}" CommandParameter="{Binding RequestMessage}" Margin="3" Style="{StaticResource SelectorItemButtonStyle}">
                                    <ToolTipService.ToolTip>
                                        <TextBlock x:Uid="Text_CopyToClipboard"/>
                                    </ToolTipService.ToolTip>
                                </Button>
                            </StackPanel>
                            <TextBlock Margin="11,5,5,5" FontSize="12" Foreground="{ThemeResource SystemControlBackgroundBaseMediumBrush}">
                                <Run x:Uid="Text_Message"/>(<Run x:Uid="Text_Hex"/>)<Run Text=":"/></TextBlock>
                            <TextBlock Grid.Column="1" Text="{Binding RequestMessage}" Style="{StaticResource RawMessageStyle}"/>
                        </Grid>
                        <Grid Grid.Row="3">
                            <Grid.Background>
                                <SolidColorBrush Color="{Binding Status, Converter={StaticResource ScanStatusColorConverter}}"/>
                            </Grid.Background>
                            <Grid Margin="6">
                                <TextBlock Margin="5,2" FontSize="12" TextWrapping="WrapWholeWords"><Run x:Uid="Text_Status" Foreground="{ThemeResource SystemControlBackgroundBaseMediumBrush}"/><Run Text=":" Foreground="{ThemeResource SystemControlBackgroundBaseMediumBrush}"/>

            </Style>
            <DataTemplate x:Key="HoldingRegisterWriterDataTemplate">
                <Grid>
                    <mtuuc:WrapPanel Visibility="{Binding IsWriteSingle, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Margin="5,6" FontSize="12" Foreground="{ThemeResource SystemControlBackgroundBaseMediumBrush}"><Run x:Uid="Text_LogicalValue"/><Run Text=":"/></TextBlock>
                            <controls:NumericBox Value="{Binding WriteValues[0].Value, Mode=TwoWay}" ValueTypeCode="Decimal"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Margin="5,4"><Run x:Uid="Text_UseMultiWrite" FontSize="12" Foreground="{ThemeResource SystemControlBackgroundBaseMediumBrush}"/><Run Text=":" FontSize="12" Foreground="{ThemeResource SystemControlBackgroundBaseMediumBrush}"/>
                                            <Run Text=" " FontFamily="Consolas"/></TextBlock>
                            <ToggleSwitch IsOn="{Binding UseMultipleWriteWhenSingle, Mode=TwoWay}" Width="80" FontSize="12" Margin="4"
                                              controls:SelectorItem.IsAutoSelectOnClick="True" Template="{StaticResource NoneAnimationToggleSwitch}"/>
                        </StackPanel>
                    </mtuuc:WrapPanel>
                    <muxc:ItemsRepeater 
                                    Margin="4" ItemsSource="{Binding WriteValues}"
                                    Visibility="{Binding IsWriteSingle, Converter={StaticResource BooleanToVisibilityInverseConverter}}">
                        <muxc:ItemsRepeater.Layout>
                            <muxc:UniformGridLayout/>
                        </muxc:ItemsRepeater.Layout>
                        <muxc:ItemsRepeater.ItemTemplate>
                            <DataTemplate>
                                <controls:NumericBox Value="{Binding Value, Mode=TwoWay}" ValueTypeCode="Decimal"/>
                            </DataTemplate>
                        </muxc:ItemsRepeater.ItemTemplate>
                    </muxc:ItemsRepeater>
                </Grid>
            </DataTemplate>
            <DataTemplate x:Key="CoilWriterDataTemplate">
                <Grid>
                    <mtuuc:WrapPanel Visibility="{Binding IsWriteSingle, Converter={StaticResource BooleanToVisibilityConverter}}">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Margin="5,6" FontSize="12" Foreground="{ThemeResource SystemControlBackgroundBaseMediumBrush}"><Run x:Uid="Text_LogicalValue"/><Run Text=":"/></TextBlock>
                            <CheckBox IsChecked="{Binding WriteValues[0].Value, Mode=TwoWay, Converter={StaticResource DecimalValueToCheckBoxConverter}}" Style="{StaticResource CoilCheckBoxNonContentStyle}"

                            </Grid>
                        </Grid>
                    </Grid>
                </Border>
            </DataTemplate>
        </Grid.Resources>
        <Pivot
            Style="{StaticResource ModbusChannelsPivotStyle}"
            ItemsSource="{Binding AppData.Channels}" 
            SelectedItem="{Binding AppData.SelectedChannel, Mode=TwoWay}"
            >
            <Pivot.ItemTemplate>
                <DataTemplate>
                    <Grid>
                        <ScrollViewer>
                            <ItemsControl ItemsSource="{Binding ModbusWriters}" IsTabStop="False" Margin="6" ItemTemplate="{StaticResource ModbusWriterDataTemplate}"/>
                        </ScrollViewer>
                        <StackPanel 
                            VerticalAlignment="Center"
                            HorizontalAlignment="Center"
                            Visibility="{Binding ModbusWriters.Count, Converter={StaticResource ObjectEqualsToVisibileConverter}, ConverterParameter={local:ToInt32 Value=0}}">
                            <StackPanel.Resources>
                                <Style TargetType="Button" BasedOn="{StaticResource AddNewItemButtonStyle}">
                                    <Setter Property="HorizontalAlignment" Value="Stretch"/>
                                    <Setter Property="HorizontalContentAlignment" Value="Left"/>
                                    <Setter Property="Margin" Value="0,3"/>
                                </Style>
                            </StackPanel.Resources>
                            <Button x:Uid="WriteDataView_AddHoldingRegisterWriter"
                                Command="{Binding ElementName=PART_WriteDataView, Path=DataContext.AddHoldingRegisterWriterCommand}"/>
                            <Button x:Uid="WriteDataView_AddCoilWriter"
                                Command="{Binding ElementName=PART_WriteDataView, Path=DataContext.AddCoilWriterCommand}"/>
                        </StackPanel>
                    </Grid>
                </DataTemplate>
            </Pivot.ItemTemplate>
        </Pivot>
    </Grid>
</UserControl>

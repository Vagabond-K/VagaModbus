                                    <Run Text="{Binding WriteValues.Count}" FontFamily="Consolas"/></TextBlock>
                            </mtuuc:WrapPanel>
                            <ContentControl Grid.Row="2" Grid.ColumnSpan="2" IsTabStop="False" Content="{Binding}" ContentTemplateSelector="{StaticResource ModbusWriterDataTemplateSelector}"/>
                            <StackPanel Grid.Column="1" Grid.RowSpan="2" Orientation="Horizontal" VerticalAlignment="Top">
                                <Button Content="&#xE70F;" Command="{Binding ElementName=PART_WriteDataView, Path=DataContext.EditWriterCommand}" CommandParameter="{Binding}" Margin="3" Style="{StaticResource SelectorItemButtonStyle}">
                                    <ToolTipService.ToolTip>
                                        <TextBlock x:Uid="ItemEditButton"/>
                                    </ToolTipService.ToolTip>
                                </Button>
                                <Button Content="&#xE74D;" Command="{Binding ElementName=PART_WriteDataView, Path=DataContext.DeleteWriterCommand}" CommandParameter="{Binding}" Margin="3" Style="{StaticResource SelectorItemButtonStyle}">
                                    <ToolTipService.ToolTip>
                                        <TextBlock x:Uid="ItemDeleteButton"/>
                                    </ToolTipService.ToolTip>
                                </Button>
                            </StackPanel>
                        </Grid>
                        <Rectangle Grid.Row="1" Height="1" Fill="{ThemeResource SystemControlForegroundBaseLowBrush}" Margin="6,0"/>
                        <Grid Grid.Row="2">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="Auto"/>
                                <ColumnDefinition/>
                                <ColumnDefinition Width="Auto"/>
                            </Grid.ColumnDefinitions>
                            <StackPanel Grid.Column="2" Orientation="Horizontal" Margin="0,0,6,0" VerticalAlignment="Top">
                                <Button Content="&#xE724;" Command="{Binding ElementName=PART_WriteDataView, Path=DataContext.ModbusWriteCommand}" CommandParameter="{Binding}" Margin="3" Style="{StaticResource SelectorItemButtonStyle}">
